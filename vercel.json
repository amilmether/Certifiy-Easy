{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "nodeVersion": "20.x",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.1.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
